<h5 class="my-tracks__ready">Welcome to <%= @manager.first_name %>'s team</h5>

<div class="container" style="max-width: 900px;">
  <div class="row">
    <% @team.each do |team_member| %>
      <div class="col-12 col-md-6 col-lg-4 d-flex justify-content-center">
        <div class="track-card mb-3">
        <strong><%= team_member.first_name %> </strong>
        </div>
      </div>
    <% end %>
  </div>
</div>